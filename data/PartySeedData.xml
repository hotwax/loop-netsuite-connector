<?xml version="1.0" encoding="UTF-8"?>
<entity-facade-xml type="ext-seed">
    <moqui.basic.EnumerationType description="Party Type" enumTypeId="PartyType"/>
    <moqui.basic.Enumeration description="Person" enumId="PtyPerson" enumTypeId="PartyType"/>
    <moqui.basic.Enumeration description="Organization" enumId="PtyOrganization" enumTypeId="PartyType"/>

    <moqui.basic.EnumerationType description="Contact Mechanism Type" enumTypeId="ContactMechType"/>
    <moqui.basic.Enumeration enumId="CmtEmailAddress" description="Email Address" enumTypeId="ContactMechType"/>
    <moqui.basic.Enumeration enumId="CmtTelecomNumber" description="Phone Number" enumTypeId="ContactMechType"/>
    <moqui.basic.Enumeration enumId="CmtPostalAddress" enumTypeId="ContactMechType" description="Postal Address"/>

    <!-- NOTE: The partyRelationshipTypeEnumId describes the FROM Party of Organization, ie Joe is a contact of ACME, so ACE
          is the fromPartyId (fromRoleTypeId= Organization) and Joe is the toPartyId (toRoleTypeId= Contact);
    -->
    <moqui.basic.EnumerationType description="Party Relationship Type" enumTypeId="PartyRelationshipType"/>
    <moqui.basic.Enumeration description="Contact" enumId="PrtContact" enumTypeId="PartyRelationshipType"/>
    <moqui.basic.StatusType description="Party Relationship" statusTypeId="PartyRelationship"/>

    <co.hotwax.netsuite.party.RoleType description="Contact" roleTypeId="Contact"/>
    <co.hotwax.netsuite.party.RoleType description="Organization" roleTypeId="Organization"/>
    <co.hotwax.netsuite.party.RoleType description="Contact Integration" roleTypeId="ContactIntegration"/>

    <ContactMechPurpose contactMechPurposeId="PostalPrimary" contactMechTypeEnumId="CmtPostalAddress"
                        description="Postal - Primary"/>
    <ContactMechPurpose contactMechPurposeId="PhonePrimary" contactMechTypeEnumId="CmtTelecomNumber"
                        description="Phone - Primary"/>
    <ContactMechPurpose contactMechPurposeId="EmailPrimary" contactMechTypeEnumId="CmtEmailAddress"
                        description="Email - Primary Address"/>
    <!-- General 'Other' purpose, mainly needed because PartyContactMech.contactMechPurposeId is a PK field, needs a non-null value -->
    <ContactMechPurpose contactMechPurposeId="Other" contactMechTypeEnumId="" description="Other"/>

    <Party partyId="_NA_" partyTypeEnumId="PtyPerson"/>
    <Person partyId="_NA_" firstName="Not" lastName="Applicable"/>

    <!-- LoopReturn Status -->
    <moqui.basic.StatusType description="Loop Return Status" statusTypeId="LoopReturn"/>
    <moqui.basic.StatusItem description="Created" sequenceNum="1" statusId="RT_CREATED" statusTypeId="LoopReturn"/>
    <moqui.basic.StatusItem description="Error" sequenceNum="2" statusId="RT_ERROR" statusTypeId="LoopReturn"/>
    <moqui.basic.StatusItem description="Pending Receipt" sequenceNum="3" statusId="RT_PENDING_RECEIPT" statusTypeId="LoopReturn"/>
    <moqui.basic.StatusItem description="Pending Refund" sequenceNum="4" statusId="RT_PENDING_REFUND" statusTypeId="LoopReturn"/>
    <moqui.basic.StatusItem description="Refunded" sequenceNum="5" statusId="RT_REFUNDED" statusTypeId="LoopReturn"/>
    <moqui.basic.StatusItem description="Processed" sequenceNum="6" statusId="RT_PROCESSED" statusTypeId="LoopReturn"/>

    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_CREATED" toStatusId="RT_ERROR" transitionName="Create Return Error"/>
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_CREATED" toStatusId="RT_PENDING_RECEIPT" transitionName="Pending Receipt Return"/>
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_PENDING_RECEIPT" toStatusId="RT_ERROR" transitionName="Pending Receipt Return Error"/>
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_PENDING_RECEIPT" toStatusId="RT_PROCESSED" transitionName="Pending Refund Return"/>
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_PROCESSED" toStatusId="RT_ERROR" transitionName="Pending Refund Return"/>
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_PROCESSED" toStatusId="RT_PENDING_REFUND" transitionName="Pending Refund Return"/>
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_PENDING_REFUND" toStatusId="RT_ERROR" transitionName="Pending Refund Return Error"/>
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_PENDING_REFUND" toStatusId="RT_REFUNDED" transitionName="Refunded Return"/>

    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_ERROR" toStatusId="RT_PENDING_RECEIPT" transitionName="Error Pending Receipt"/>
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_ERROR" toStatusId="RT_PENDING_REFUND" transitionName="Error Pending Refund"/>
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_ERROR" toStatusId="RT_REFUNDED" transitionName="Error Refunded Return"/>
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="RT_ERROR" toStatusId="RT_PROCESSED" transitionName="Error Processed Return"/>
</entity-facade-xml>